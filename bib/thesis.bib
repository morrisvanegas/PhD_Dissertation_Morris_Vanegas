journal = {NeuroImage},
keywords = {Brain,Brain: physiology,Cerebral Cortex,Cerebral Cortex: metabolism,Cerebral Cortex: pathology,Functional Neuroimaging,Functional Neuroimaging: methods,Humans,Mental Disorders,Mental Disorders: pathology,Mental Disorders: psychology,Mental Disorders: therapy,Psychiatry,Psychiatry: instrumentation,Psychiatry: methods,Psychotic Disorders,Psychotic Disorders: diagnosis,Psychotic Disorders: pathology,Psychotic Disorders: therapy,Spectroscopy, Near-Infrared,Spectroscopy, Near-Infrared: methods},
pages = {478--88},
