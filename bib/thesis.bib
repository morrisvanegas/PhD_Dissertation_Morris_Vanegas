keywords = {*Magnetic Resonance Imaging,3D visualization,Computer Simulation,Fingers/*diagnostic imaging,Humans,Imaging, Three-Dimensional,MR microscopy,MRI,Magnetic Resonance Spectroscopy,Models, Anatomic,Pacinian corpuscle,Phantoms, Imaging,RF coil,Radio Waves,Signal-To-Noise Ratio,human finger,ligaments,solenoid,tendons,ultrahigh field,vasculature},
language = {eng},
