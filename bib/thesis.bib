number = {6},
pages = {602--610},
title = {{New Aspects of Breast Volume Measurement Using 3-Dimensional Surface Imaging}},
url = {https://journals.lww.com/annalsplasticsurgery/Fulltext/2006/12000/New_Aspects_of_Breast_Volume_Measurement_Using.3.aspx},
volume = {57},
year = {2006}
}
