author = {Hendrick, R Edward},
doi = {10.1148/radiol.10100570},
issn = {0033-8419},
journal = {Radiology},
month = {oct},
number = {1},
pages = {246--253},
publisher = {Radiological Society of North America},
title = {{Radiation Doses and Cancer Risks from Breast Imaging Studies}},
