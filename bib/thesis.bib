title = {{Monte Carlo simulation of photon migration in 3D turbid media accelerated by graphics processing units}},
url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=19997242},
volume = {17},
year = {2009}
