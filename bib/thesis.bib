doi = {10.1007/s00266-011-9708-x},
file = {:Users/morrisvanegas/Library/Application Support/Mendeley Desktop/Downloaded/Koch et al. - 2011 - Breast volumetry using a three-dimensional surface assessment technique.pdf:pdf},
issn = {0364216X},
journal = {Aesthetic Plastic Surgery},
