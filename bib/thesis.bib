issn = {1083-3668},
journal = {Journal of Biomedical Optics},
keywords = {18,2,20,2013,5,accepted for publication aug,breast cancer,diffuse optical tomography,digital diffuse optical imaging,dynamic optical imaging,online sep,optical breast imaging,paper 130214r received apr,published,revised manuscript received jul},
number = {9},
pages = {096012},
pmid = {24048367},
title = {{Optical biomarkers for breast cancer derived from dynamic diffuse optical tomography}},
