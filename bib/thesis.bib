keywords = {Cortical function,Motor recovery,Near-infrared spectroscopy,Stroke,Systemic review},
number = {FEB},
pages = {1--14},
title = {{A systemic review of functional near-infrared spectroscopy for stroke: Current application and future directions}},
volume = {10},
year = {2019}
