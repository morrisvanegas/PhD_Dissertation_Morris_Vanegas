pages = {2366--2379},
publisher = {Optical Society of America},
title = {{Characterization of structural-prior guided optical tomography using realistic breast models derived from dual-energy x-ray mammography}},
url = {https://pubmed.ncbi.nlm.nih.gov/26203367 https://www.ncbi.nlm.nih.gov/pmc/articles/PMC4505695/},
volume = {6},
